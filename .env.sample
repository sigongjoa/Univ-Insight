# Univ-Insight Environment Variables Sample
# Copy this file to .env and fill in the values

# 1. CareerNet API (https://www.career.go.kr/cnet/front/openapi/openApiMain.do)
# 회원가입 후 인증키 신청 필요
CAREERNET_API_KEY=your_api_key_here

# 2. Database
DATABASE_URL=sqlite:///./univ_insight.db
DB_ECHO=false

# 3. LLM (Ollama)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=qwen2.5:latest

# 4. API Server
API_HOST=0.0.0.0
API_PORT=8000
API_DEBUG=true
